class Solution {
public:
    int prefixCount(vector<string>& words, string pref) {
        int count = 0;
        for(string &s : words){
            string c = "";
            for(char d : s){
                c += d;
                if(c == pref){
                    count++;
                    break;
                }
            }
        }
        return count;
    }
};
