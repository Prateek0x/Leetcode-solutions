class Solution {
public:
    int thirdMax(vector<int>& nums) {

        set<int> s;
        for (int i = 0; i < nums.size(); i++) {
            s.insert(nums[i]);
        }
        vector<int> v(s.begin(), s.end());
        if (s.size() <= 2)
            return v[s.size() - 1];
        return v[v.size()-3];
    }
};
