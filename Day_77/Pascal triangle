class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans;
        for(int n = 0; n < numRows; n++){
        vector<int>row;
        long long ansval = 1;

        row.push_back(1);

        for(int i = 1; i <= n; i++){
            ansval = ansval*(n - i+1) / i;
            row.push_back(ansval);
        }
        ans.push_back(row);
    }
    return ans;
}
};
