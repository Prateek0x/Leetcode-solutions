class Solution {
public:

    int c=0;
    int func(int i,int n,vector<int>& dp){
        if(i==n){
            return 1;
        }
        if(i>n){
            return 0;
        }
        if(dp[i]!=-1){
            return dp[i];

        }
       return dp[i]=func(i+1,n,dp)+func(i+2,n,dp);
    }

    int climbStairs(int n) {
        if(n==1){
            return 1;
        }
         vector<int> dp(n+1,-1);
        int a=func(0,n,dp);
       
        return a;
    }
};
