class Solution {
public:
    vector<int> sumZero(int n) {
        int el = n / 2, i = 0;
        vector<int> output(n);

        if (n % 2 != 0) { n -= 1; }

        while (i < n)
        {
            output[i] = el;
            i++; el--;
            if (el == 0) { el -= 1; }
        }

        return output;
    }
};
